func doTrim(_data)
  fvar(f)
  f = isdef(Place) ? '' : 'l'
  if(linked(Result))
    println(rslt, ' = ' + f + 'trim(', Value, ');')
    event(onTrim, rslt)
  else
    event(onTrim, f + 'trim(' && Value && ')')
  end 
end

func Result()
  fvar(f)
  f = isdef(Place) ? '' : 'l'
  if(linked(doTrim))
    return(rslt)
  else
    return(f + 'trim(' && Value && ')')
  end
end

