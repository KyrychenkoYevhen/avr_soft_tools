section java

func create(_data)
  return(Parent && '.lastChild')
end

func doGetLastChild(_data)
  if(linked(Child))
    println(ln, ' = ', create(_data), ';')
    event(onGetLastChild, ln)
  else
    event(onGetLastChild, create(_data))
  end
end

func Child(_data)
  if(linked(doGetLastChild))
    return(ln)
  else
    return(create(_data))
  end
end

